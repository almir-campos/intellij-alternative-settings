    <extensionMap>
      <mapping ext="mp3" type="Native" />
      <mapping ext="wav" type="Native" />
      <mapping pattern="CALIBRATION-LUA" type="PLAIN_TEXT" />
      <mapping pattern="FilesLost!" type="PLAIN_TEXT" />
      <mapping ext="bak" type="PLAIN_TEXT" />
      <mapping ext="hprof" type="PLAIN_TEXT" />
      <removed_mapping ext="txt" approved="true" type="PLAIN_TEXT" />
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping pattern=".gitignore_global" type="Git file" />
    </extensionMap>
  </component>
</application>